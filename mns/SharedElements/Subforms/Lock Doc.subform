		'If Source.Document.DisableEditDocument(0) = "1" Then
           '*** блокируем документ
			'Call LockEdition( Source, flag )
		'End If
	
	'End If
	Exit Sub
ErrHnd:  
	Call errh(" Lock Doc Querysave ")
End Sub
'++LotusScript Development Environment:2:2:Postopen:1:12
Sub Postopen(Source As Notesuidocument)
	On Error Goto ErrHnd	
'	Exit Sub
	Dim EditPermit As Variant
	qunid = source.Document.UniversalID
     '*** если новый документ, то блокировать не надо.     
	If Source.IsNewDoc Then Exit Sub
	
     '*** если документ открывается для чтения, то блокировать не надо.     
	If Source.EditMode = False Then Exit Sub 
	
     '*** если флаг блокировки установлен
	If Source.Document.DisableEditDocument(0) = "1" Then
          '*** пытаемся блокировать документ
		Call LockEdition( Source, EditPermit )
		DeleteLockDoc = EditPermit
	End If
	Exit Sub
ErrHnd:  
	Call errh(" Lock Doc Postopen ")
End Sub
'++LotusScript Development Environment:2:2:LockEdition:1:8
Sub LockEdition(uid As NotesUIDocument, EditPermit As Variant)
     '*** проверяет заблокирован ли документ; если нет, то блокирует, иначе выдает сообщение
	
	
	                          'для получения информации о текущем редакторе
	Dim formula As String                                      'формула для поиска блокирующего документа  
	Dim dc As NotesDocumentCollection             'коллекция блокирующих доокументов
	Dim un As NotesName                                    'имя пользователя (для сообщения)
	Dim other As NotesDocument
	Dim s As  New NotesSession
	Dim db As NotesDatabase
	Dim view As NotesView
	Dim ldoc As NotesDocument
	
	Set db = s.CurrentDatabase
	
	EditPermit = True                                                        '*** редактирование разрешено
	
     '*** получение коллекции блокирующих документов
	Set view = db.GetView("Lock Doc")
	Call view.Refresh
	Set ldoc = view.GetDocumentByKey(uid.Document.UniversalID)
	
	If Not(ldoc Is Nothing)  Then
		
'		If ldoc.LockUser(0) &lt;&gt; s.UserName Then ' 2015/05/07 идиоты умудряются открывать свой документ на редакцию несколько раз, пример заявка из VOKC 
		Set un = New NotesName(ldoc.LockUser(0))
		Messagebox "В данный момент документ редактирует :  " + un.Common +_
		". Документ будет открыт для чтения.", 0 + 48, "Сообщение"
		
		uid.EditMode = False
		EditPermit = False
'		End If
		
	Else
		Set ldoc = db.CreateDocument
		ldoc.Form = "LockDoc"
		ldoc.LockID = uid.Document.UniversalID
		ldoc.LockUser = s.UserName
		Dim AuthorsList As New NotesItem( ldoc,"UnlockUsers", "[Administrator]", AUTHORS )
		Call AuthorsList.AppendToTextList( s.UserName )
		
          '*** сохраняем документ
		Call ldoc.Save(True, False)
		
          '*** проверяем был ли создан ранее блокирующий документ с LockID
		Set dc = view.GetAllDocumentsByKey( uid.Document.UniversalID, True )
		
		If dc.Count &gt; 1 Then                                 '*** если да, то удаляем вновь созданный 
			Set other = dc.GetFirstDocument
			
			Do While Not other Is Nothing 
				
				If other.LockUser(0) &lt;&gt; s.UserName Then
					uid.Document.EditMode = False
					Set un = New NotesName(ldoc.LockUser(0))
					Messagebox "В данный момент документ редактирует: " + un.Common +_
					". Документ будет открыт для чтения.", 0 + 48, "Сообщение"
					
					Call ldoc.Remove(False)
					EditPermit = False
					Exit Do
				End If
				
				Set other = dc.GetNextDocument( other )
			Loop
			
		End If
		
	End If
End Sub


'++LotusScript Development Environment:2:2:UnlockEdition:1:8
Sub UnlockEdition( unid ) ' uid As NotesUIDocument )
     '*** разблокирует документ, если вызывающий процедуру - редактор
	
	Dim formula As String
	Dim dc As NotesDocumentCollection
	
'	If uid.EditMode = False Then Exit Sub
	
	Dim s As  New NotesSession
	Dim db As NotesDatabase
	Dim view As NotesView
	Dim ldoc As NotesDocument
	
	Set db = s.CurrentDatabase
	
	Set view = db.GetView("Lock Doc")
	Call view.Refresh
	Set ldoc = view.GetDocumentByKey(unid) ' (uid.Document.UniversalID)
	
	If Not(ldoc Is Nothing)  Then
          '*** проверяем, кто редактирует
		
		If ldoc.LockUser(0) = s.UserName Then
			Call ldoc.Remove(True)
		End If
		
	End If
	
End Sub<break/></text></item>
