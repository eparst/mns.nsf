<item name='$$FormScript' summary='false' sign='true'><text>'++LotusScript Development Environment:2:5:(Options):0:66

'++LotusScript Development Environment:2:5:(Forward):0:1
Declare Sub Querymodechange(Source As Notesuidocument, Continue As Variant)
Declare Sub Queryclose(Source As Notesuidocument, Continue As Variant)
Declare Sub Querysave(Source As Notesuidocument, Continue As Variant)
Declare Sub Postopen(Source As Notesuidocument)
Declare Sub LockEdition(uid As NotesUIDocument, EditPermit As Variant)
Declare Sub UnlockEdition( unid ) ' uid As NotesUIDocument )

'++LotusScript Development Environment:2:5:(Declarations):0:2


'++LotusScript Development Environment:2:2:BindEvents:1:129
Private Sub BindEvents(Byval Objectname_ As String)
	Static Source As NOTESUIDOCUMENT
	Set Source = Bind(Objectname_)
	On Event Querymodechange From Source Call Querymodechange
	On Event Queryclose From Source Call Queryclose
	On Event Querysave From Source Call Querysave
	On Event Postopen From Source Call Postopen
End Sub

'++LotusScript Development Environment:2:2:Querymodechange:1:12
Sub Querymodechange(Source As Notesuidocument, Continue As Variant)
	On Error Goto ErrHnd	
'	Exit Sub
	If Not(Source.EditMode) Then
		
          '*** если флаг блокировки установлен, то пытемся блокировать документ
		If Source.Document.DisableEditDocument(0) = "1" Then
			Call LockEdition( Source, Continue )
			
			' Поиск документа
			Dim ses As New NotesSession, ws As New NotesUIWorkspace, view As NotesView, doc As NotesDocument
			Set view = ses.CurrentDatabase.GetView( "NoteID" )
			Set doc = Source.Document
			v = Evaluate( "@NoteID", doc )
'			s$ = v(0)
			Set doc = view.GetDocumentByKey( v(0)) 's$ )
			
			Dim LastModified As New NotesDateTime( doc.LastModified )
			Dim CurrentModified As New NotesDateTime( Source.Document.LastModified )
			DeleteLockDoc = True
			
			If CurrentModified.TimeDifference( LastModified ) &lt; 0 Then
				' Заново открываем документ
				Call Source.Close
				DeleteLockDoc = False
				Call ws.EditDocument( Continue, doc )
			End If
			
		End If
		
	End If
	Exit Sub
ErrHnd:  
	Call errh(" Lock Doc Querymodechange ")
End Sub
'++LotusScript Development Environment:2:2:Queryclose:1:12
Sub Queryclose(Source As Notesuidocument, Continue As Variant)
	On Error Goto ErrHnd	
'	Exit Sub
'	If Source.EditMode Then
	If DeleteLockDoc Then Call UnlockEdition( qunid )
	DeleteLockDoc = True
'	End If
	Exit Sub
ErrHnd:  
	Call errh(" Lock Doc Queryclose ")
End Sub
'++LotusScript Development Environment:2:2:Querysave:1:12
Sub Querysave(Source As Notesuidocument, Continue As Variant)
	On Error Goto ErrHnd	
