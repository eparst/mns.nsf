<item name='$$FormScript' summary='false' sign='true'><text>'++LotusScript Development Environment:2:5:(Options):0:66

'++LotusScript Development Environment:2:5:(Forward):0:1
Declare Sub Postopen(Source As Notesuidocument)
Declare Sub Querysave(Source As Notesuidocument, Continue As Variant)
Declare Sub Postsave(Source As Notesuidocument)

'++LotusScript Development Environment:2:5:(Declarations):0:2

'++LotusScript Development Environment:2:2:BindEvents:1:129
Private Sub BindEvents(Byval Objectname_ As String)
	Static Source As NOTESUIDOCUMENT
	Set Source = Bind(Objectname_)
	On Event Postopen From Source Call Postopen
	On Event Querysave From Source Call Querysave
	On Event Postsave From Source Call Postsave
End Sub

'++LotusScript Development Environment:2:2:Postopen:1:12
Sub Postopen(Source As Notesuidocument)
	On Error Goto ErrHnd	
'	Exit Sub
	Dim db As NotesDatabase
	Dim view As NotesView
	'Dim doc As NotesDocument
	Dim agent As NotesAgent
	Dim FromAccess As DocumentAccess2
	Dim doc As NotesDocument	
	
	Set doc = Source.Document
	UserCategory = doc.UserCategory
	IsNewDoc = Source.IsNewDoc
	
	If IsNewDoc Then
		
		Dim ses As New NotesSession
		Set db = ses.CurrentDatabase
		flag = doc.Source(0) = "0"
		If EditAccess Is Nothing Then Set EditAccess = New DocumentAccess2( doc, "Editors", "AuthorNames" )
		If ReadAccess Is Nothing Then Set ReadAccess = New DocumentAccess2( doc, "Readers", "ReaderNames" )
		EditAccess.ExtraParam = EP_AUTHORS
		ReadAccess.ExtraParam = EP_READERS
		
		' Документ настройки
		Set db = doc.ParentDatabase
		Set dbmain = New NotesDatabase( db.Server, Left( db.FilePath, Len( db.FilePath ) - Len( db.FileName ) ) + "main.nsf" )
		Set view = dbmain.GetView( "TypeDocPermission" )
		Set tunedoc = view.GetDocumentByKey( doc.TypeDoc(0) )
		
		If tunedoc Is Nothing Then
			Call EditAccess.AddAuthor
			Call Source.Reload
			Exit Sub
		End If
		
		' Редакторы
		If flag Then
			If tunedoc.Permission(0) = "1" Then
				'permission = tunedoc.PermissionList
				'If Not Isnull( Arraygetindex( permission, "0" ) ) Then Call EditAccess.AddDepart
				'If Not Isnull( Arraygetindex( permission, "1" ) ) Then Call EditAccess.AddChiefs
				'If Not Isnull( Arraygetindex( permission, "2" ) ) Then Call EditAccess.AddResponsibles
			Else
				' Автор
				Call EditAccess.AddAuthor
			End If
		Else
			' Автор
			Call EditAccess.AddAuthor
		End If
		
		' Читатели
		If tunedoc.Permission_R(0) = "1" Then
			If flag Then
				'permission = tunedoc.PermissionList_R
				'If Not Isnull( Arraygetindex( permission, "0" ) ) Then Call ReadAccess.AddDepart
				'If Not Isnull( Arraygetindex( permission, "1" ) ) Then Call ReadAccess.AddChiefs
				'If Not Isnull( Arraygetindex( permission, "2" ) ) Then Call ReadAccess.AddResponsibles
			End If
		Else
			If tunedoc.ReadDefault_R(0) = "1" Then
				Call ReadAccess.DeleteAll( False )
				Call ReadAccess.UpdateEmployees( "Editors" )
				Call ReadAccess.AddAuthor
				Call ReadAccess.SetAccess
			End If
		End If
		Call Source.Reload
		
		' Отправитель.
		If doc.TypeDoc(0) = "DocOut" Or doc.TypeDoc(0) = "RespDocOut" Then
			Set FromAccess = New DocumentAccess2( doc, "FromInfo", "FromNotesAddress" )
			FromAccess.AddAuthor
			FromAccess.SetAccess
			doc.From = Word( FromAccess.GetEmployeeLine( 0 ), 1, "!" )
			doc.FromAddress = "-"
			
			Call Source.Reload
			Call Source.Refresh
		End If
		
	End If
	
	EditorsInfo = Source.Document.Editors
	ReadersInfo = Source.Document.Readers
	Exit Sub
ErrHnd:  
	Call errh(" Permission Postopen ")
End Sub
'++LotusScript Development Environment:2:2:Querysave:1:12
Sub Querysave(Source As Notesuidocument, Continue As Variant)
	On Error Goto ErrHnd	
'	Exit Sub
	Dim ses As New NotesSession
	Dim db As NotesDataBase
	Dim agent As NotesAgent
	Dim dc As NotesDocumentCollection
	Dim pdb As NotesDatabase		' база данных родительского документа
	Dim pdoc As NotesDocument 	' родительский документ
	
	' Проверка изменения списков доступа
	If CompareArrays( EditorsInfo, Source.Document.Editors ) = 1 Then
		If CompareArrays( ReadersInfo, Source.Document.Readers ) = 1 Then Exit Sub
	End If
	
	If EditAccess Is Nothing Then
		Set EditAccess = New DocumentAccess2( Source.Document, "Editors", "AuthorNames" )
	Else
		EditAccess.Update
	End If
	
	If ReadAccess Is Nothing Then
		Set ReadAccess = New DocumentAccess2( Source.Document, "Readers", "ReaderNames" )
	Else
		ReadAccess.Update
	End If
	
	' Документ настройки по типу документа
	If tunedoc Is Nothing Then
		If dbmain Is Nothing Then Set dbmain = New NotesDatabase( EditAccess.Server, EditAccess.DBMain )
		Set view = dbmain.GetView( "TypeDocPermission" )
		Set tunedoc = view.GetDocumentByKey( Source.Document.TypeDoc(0) )
		If tunedoc Is Nothing Then Exit Sub
	End If
	
	' Редакторы
	If Source.Document.AuthorNames(0) &lt;&gt; "[AllAuthors]" Then
		'If tunedoc.Permission(0) = "1" And Not Isnull( Arraygetindex( tunedoc.PermissionList, "1" ) ) Then
			'If tunedoc.EditCheef(0) = "0" Then Call EditAccess.AddChiefs
		'End If
	End If
	
	' Читатели
	'If tunedoc.Permission_R(0) = "1" And _
	'( Source.Document.ReaderNames(0) &lt;&gt; "" And Not  Source.IsNewDoc ) Then
		'If Not Isnull( Arraygetindex( tunedoc.PermissionList_R, "1" ) ) Then
			'If tunedoc.EditCheef_R(0) = "0" Then Call ReadAccess.AddChiefs
		'End If
	'End If
	
	Exit Sub
ErrHnd:  
	Call errh(" Permission Querysave ")
End Sub
'++LotusScript Development Environment:2:2:Postsave:1:12
Sub Postsave(Source As Notesuidocument)
	On Error Goto ErrHnd	
'	Exit Sub
	Dim ses As New NotesSession
	Dim db As NotesDataBase
	Dim agent As NotesAgent
	Dim dc As NotesDocumentCollection
	Dim pdb As NotesDatabase		' база данных родительского документа
	Dim pdoc As NotesDocument 	' родительский документ
	Dim doc As NotesDocument
	Dim change As Variant			' флаг изменения списков доступа
	
	Set doc = Source.Document
	
	' Проверка изменения списков доступа
	change = True
	If CompareArrays( EditorsInfo, doc.Editors ) = 1 Then
		If CompareArrays( ReadersInfo, doc.Readers ) = 1 Then
			' Список доступа к документу не изменились
			change = False
		End If
	End If
	
	' Если списки доступа не изменялись и документ был создан ранее, то:
	If Not ( change Or IsNewDoc ) Then Exit Sub
	
	If change Then
		EditorsInfo = doc.Editors
		ReadersInfo = doc.Readers
	End If
	
	If EditAccess Is Nothing Then
		Set EditAccess = New DocumentAccess2( doc, "Editors", "AuthorNames" )
	Else
		EditAccess.Update
	End If
	
	If ReadAccess Is Nothing Then
		Set ReadAccess = New DocumentAccess2( doc, "Readers", "ReaderNames" )
	Else
		ReadAccess.Update
	End If
	
	' Документ настройки базы данных
	Set db = ses.CurrentDatabase
	If dbmain Is Nothing Then Set dbmain = New NotesDatabase( EditAccess.Server, EditAccess.DBMain )
	Set view = dbmain.GetView( "Setup" )
	Set tunedb = view.GetDocumentByKey( Ucase( db.FileName ) )
	If tunedb Is Nothing Then Exit Sub
	
	' Наследование списков из родительского документа
	If doc.IsResponse And IsNewDoc Then	'&lt;--- Документ ответный и новый
		Set pdb = Source.Document.Parentdatabase
		Set pdoc = pdb.GetDocumentByUnid( doc.ParentDocumentUnid )
		If tunedoc.HasItem( "AutoReadHierarchy" ) And tunedoc.AutoReadHierarchy(0) = "1" Then	'&lt;--- Автоматически
			If tunedoc.InheritedList(0) = "1" Or tunedoc.ViewAllTree(0) = "1" Then	'&lt;--- Наследование или все дерево
				If doc.HasItem( "ReaderNames" ) And doc.ReaderNames(0) &lt;&gt; "" Then '&lt;----- Не могут читать все
					' Редакторы
					Forall info In pdoc.Editors
						If ReadAccess.FindEmployee( WordLeft( info, 2, "!") ) &lt; 0 Then ReadAccess.AddEmployeeLine info, False
					End Forall
					' Читатели
					Forall info In pdoc.Readers
						If ReadAccess.FindEmployee( WordLeft( info, 2, "!" ) ) &lt; 0 Then ReadAccess.AddEmployeeLine info, False
					End Forall
					ReadAccess.SetAccess
					Call doc.Save( True, True )
					Call Source.Reload
				End If
			End If
		End If
	End If
	
	' Дописать в родительский документ списки доступа из ответного
	If IsNewDoc Or change Then	'&lt;--- Документ новый или были изменены списки читателей и(или) редакторов
		If tunedb.HasItem( "AutoReadHierarchy" ) And tunedb.AutoReadHierarchy(0) = "1" Then
			doc.ViewAllTree = tunedb.ViewAllTree(0)
			Call doc.Save( True, True )
			Set agent = ses.CurrentDatabase.GetAgent( "(SetResponsAccess)" )
			Call agent.RunOnServer( doc.NoteID )
		End If
	End If
	IsNewDoc = False
	Exit Sub
ErrHnd:  
	Call errh(" Permission Postsave ")
End Sub<break/></text></item>
